function mkd() {
	mkdir -p "$@" && cd "$@"
}

function my_ip {
	ifconfig | grep 'broadcast' | awk '{print $6}'
}


get_git_branch_string() {
	  git branch 2> /dev/null | sed -e '/^[^*]/d' -e 's/* \(.*\)/\1/'
}
pull() {
	  git pull origin $(get_git_branch_string)
}
push() {
	  git push origin $(get_git_branch_string)
}
